{
  "hosting": {
    "site": "gomez-brand-store",
    "public": ".output/public",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/.git/**",
      "**/.github/**"
    ],
    "rewrites": [{ "source": "**", "function": "server" }]
  },
  "functions": { "source": ".output/server" }
}
